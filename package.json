{
  "name": "geolocation-browsers-test",
  "version": "1.0.3",
  "description": "`npm install` or `yarn` `npm start` or `yarn start`",
  "main": "./dist/App.js",
  "directories": {
    "test": "./setup/enzyme"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint",
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test || npm test"
    }
  },
  "scripts": {
    "build": "webpack --config ./setup/webpack/webpack.prod.js",
    "prestart": "cp -f ./certs/localhost.pem ./node_modules/webpack-dev-server/ssl || :",
    "start": "webpack-dev-server --config ./setup/webpack/webpack.dev.js",
    "test": "jest",
    "format": "prettier -c --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint src"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "./setup/enzyme/enzyme.setup.js"
    ]
  },
  "author": "Leandro Costa",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "uuid": "^3.3.2"
  },
  "resolutions": {
    "**/react": "16.8.6",
    "**/react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^7.0.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.1"
  }
}
